{
  "environment_parameters": {
    "app_name" : "example-view-rawdata",
    "problem_type" : "seq2class",
    "environment_id" : "console_localhost",
    "output_folder": "outputs",
    "logging_level" : "info",
    "view_plots" : false,
    "save_plots" : true,
    "register_mlflow": false
  },

  "raw_data_parameters": {
    "dataset_goal":"supervised_learning",
    "data_source":"localhost_datafile",
    "data_file_path":"static/raw_data/xploreML_sample/xploreML_rawdata_sample.txt",
    "separator":"\t",
    "perc_load":1.0,
